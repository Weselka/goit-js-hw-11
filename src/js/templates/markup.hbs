{{#each this}}
  <div class='photo-card'>
    <a class="gallery__link gallery__item" href='{{userImageURL}}'>
      <img class='gallery__image' width='240' src='{{webformatURL}}' alt='{{tags}}' loading='lazy' />
    </a>
    <div class='info'>
      <p class='info-item'>
        <b>Likes</b>{{likes}}
      </p>
      <p class='info-item'>
        <b>Views</b>{{views}}
      </p>
      <p class='info-item'>
        <b>Comments</b>{{comments}}
      </p>
      <p class='info-item'>
        <b>Downloads</b>{{downloads}}
      </p>
    </div>
  </div>
{{/each}}